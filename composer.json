  "keywords": ["Doctrine", "DBAL", "DateTime", "datetime", "UTC"],
  "authors": [
    {
      "name": "Austin Morris",
      "email": "austin.morris@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.2",
    "doctrine/dbal": "~2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "4.*"
  },
  "autoload": {
    "psr-4": {
